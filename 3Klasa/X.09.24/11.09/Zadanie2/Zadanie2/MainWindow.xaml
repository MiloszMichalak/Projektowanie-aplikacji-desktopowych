<Window x:Class="Zadanie2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zadanie2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="92,53,0,0" TextWrapping="Wrap" Text="Imie" VerticalAlignment="Top" Width="120" Name="Imie"/>
        <RadioButton Content="Kobieta" HorizontalAlignment="Left" Margin="92,90,0,0" VerticalAlignment="Top" Name="male"/>
        <RadioButton Content="Mezczyzna" HorizontalAlignment="Left" Margin="92,110,0,0" VerticalAlignment="Top" Name="female"/>
        <CheckBox Content="Pociagi" HorizontalAlignment="Left" Margin="97,140,0,0" VerticalAlignment="Top" Name="pociag"/>
        <CheckBox Content="Samoloty" HorizontalAlignment="Left" Margin="97,160,0,0" VerticalAlignment="Top" Name="samolot"/>
        <TextBox HorizontalAlignment="Left" Margin="92,199,0,0" TextWrapping="Wrap" Text="Cos o sobie" VerticalAlignment="Top" Width="297" Height="79" Name="oSobie"/>
        <Button Content="zapisz" HorizontalAlignment="Left" Margin="91,312,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.147,0.157" Click="Button_Click"/>
        <Button Content="wyjdz" HorizontalAlignment="Left" Margin="290,317,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.057,-0.936"/>

    </Grid>
</Window>
